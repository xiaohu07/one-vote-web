{
  "name": "one-note-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "node --max_old_space_size=4097 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.js --open --aot",
    "start": "NODE_ENV=development nodemon server.js & npm run serve",
    "compile": "ng build --prod --aot true --build-optimizer true --source-map false --progress false",
    "build": "npm run compile && rm -rf build && mkdir -p build/client && cp -R dist/* build/client/ && cp manifest.yml build/manifest.yml && cp package.json build/package.json && cp package-lock.json build/package-lock.json && cp server.js build/server.js && cp src/favicon.jpg build/client/favicon.jpg && rm -rf dist && cp Dockerfile build/Dockerfile && cd build && npm install --production --quiet --no-bin-links",
    "test": "jest --watch",
    "test-ci": "jest --coverage --silent",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch"
  },
  "private": true,
  "dependencies": {
    "express": "^4.15.4",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.3.4",
    "@angular/cli": "7.1.4",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/compiler-cli": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "7.2.0",
    "@swimlane/ngx-charts": "^10.0.0",
    "@types/file-saver": "1.3.0",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "~4.3.0",
    "core-js": "^2.6.1",
    "d3": "^5.7.0",
    "enhanced-resolve": "^4.0.0",
    "file-saver": "^1.3.3",
    "formdata-polyfill": "^3.0.10",
    "jest": "^24.1.0",
    "jest-preset-angular": "^6.0.2",
    "jsdom": "^13.1.0",
    "ngx-webstorage": "^3.0.0-beta.14",
    "nodemon": "^1.17.5",
    "rxjs": "^6.2.1",
    "ts-jest": "^23.10.5",
    "ts-node": "~6.1.2",
    "tslib": "^1.9.0",
    "tslint": "~5.10.0",
    "typescript": "3.2.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": "8.x.x"
  },
  "jest": {
    "testURL": "http://localhost",
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 75,
        "functions": 95
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/setupJest.ts",
      "<rootDir>/src/jestGlobalMocks.ts",
      "<rootDir>/node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/setupJest.ts",
      "<rootDir>/src/jestGlobalMocks.ts",
      "<rootDir>/node_modules"
    ]
  }
}
